{
  "name": "video-downloader",
  "version": "1.0.0",
  "description": "Modern React video downloader with beautiful UI",
  "private": true,
  "dependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "react-router-dom": "^6.20.1",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "web-vitals": "^2.1.4",
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "concurrently \"npm run backend\" \"react-scripts start\"",
    "backend": "cd backend && npm start",
